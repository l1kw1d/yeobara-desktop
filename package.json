{
  "name": "yeobara-desktop",
  "productName": "yeobara-desktop",
  "version": "0.0.0",
  "description": "yobara(`여봐라` in Korea), which mean is almost the same meaning of `Excuse me. I'm here` in English",
  "license": "MIT",
  "repository": "yeobara/yeobara-desktop",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
    "url": "ragingwind.md"
  },
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "0.34.0",
  "scripts": {
    "test": "xo",
    "start": "electron ./src/platform",
    "postinstall": "npm run dotenv",
    "build": "electron-packager ./src/platform $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --all --version=$npm_package_electronVersion",
    "bundle": "browserify src/lib/bundle.js -o src/app/scripts/bundle.js",
    "dotenv": "dotenv-touch --process=FB_APIKEY,FB_APPNAME --output='.env'"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "dotenv": "^1.2.0",
    "eddystone-beacon": "^1.0.3",
    "electron-debug": "^0.4.0",
    "electron-menu-loader": "0.0.3",
    "hasha": "^2.0.2",
    "hyphenize": "0.0.2",
    "node-gcm": "^0.12.1"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "dotenv-touch": "^0.1.0",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.34.0",
    "xo": "^0.9.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  },
  "jshintConfig": {
    "esnext": true,
    "node": true
  }
}
